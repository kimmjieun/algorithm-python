        "def cnt(n):\r\n",
        "  fib=[0 for _ in range(n+1)]\r\n",
        "  if n ==0 or n==1:\r\n",
        "    return 1\r\n",
        "\r\n",
        "  fib[0]=1\r\n",
        "  fib[1]=1\r\n",
        "  for i in range(2,n+1):\r\n",
        "    fib[i]=fib[i-1]+2*fib[i-2]\r\n",
        "  return fib[n]\r\n",
        "\r\n",
        "\r\n",
        "while True:\r\n",
        "    try:\r\n",
        "        print(cnt(int(input())))\r\n",
        "    except:\r\n",
        "        break"
      ],
