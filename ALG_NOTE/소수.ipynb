        "def isPrime(a):\r\n",
        "  if a<2:\r\n",
        "    return False\r\n",
        "  else:\r\n",
        "    for i in range(2,a):\r\n",
        "      if a%i==0:\r\n",
        "        return False\r\n",
        "    return True"
      ],
