      "source": [
        "#1912번 연속합\r\n",
        "\r\n",
        "n = int(input())\r\n",
        "\r\n",
        "arr = list(map(int,input().split()))\r\n",
        "dp = [0 for _ in range(n)]\r\n",
        "dp[0]=arr[0]\r\n",
        "for i in range(1,n):\r\n",
        "  dp[i]=max(dp[i-1]+arr[i],arr[i])\r\n",
        "print(max(dp))"
      ],
