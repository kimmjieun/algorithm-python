      "source": [
        "# 1463번 1로 만들기\r\n",
        "\r\n",
        "n = int(input())\r\n",
        "dp = [0 for _ in range(n+1)]\r\n",
        "\r\n",
        "for i in range(2,n+1):\r\n",
        "  dp[i]=dp[i-1]+1\r\n",
        "\r\n",
        "  if i%3 ==0 and  dp[i//3]+1<dp[i]:\r\n",
        "    dp[i]=dp[i//3]+1\r\n",
        "\r\n",
        "  if i%2 ==0 and dp[i//2]+1<dp[i]:\r\n",
        "    dp[i]=dp[i//2]+1\r\n",
        "print(dp[n])\r\n"
      ],
